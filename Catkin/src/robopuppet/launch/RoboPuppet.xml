<!-- RoboPuppet Launch File -->
<!-- Launches Baxter simulation and RoboPuppet serial interface -->
<launch>

	<!-- Baxter Launch File -->
	<include file="$(find baxter_gazebo)/launch/baxter_world.launch"/>

	<!-- RoboPuppet Node -->
	<node pkg="robopuppet" type="robopuppet.py" name="RoboPuppet" output="screen"/>
	
</launch>
