<!-- RoboPuppet Launch File -->
<!-- Launches Baxter simulation and RoboPuppet serial interface -->
<launch>

	<!-- Baxter Launch File -->
	<include file="$(find baxter_gazebo)/launch/baxter_world.launch"/>

	<!-- RoboPuppet Node -->
	<node pkg="robopuppet" type="RoboPuppet.py" name="RoboPuppet"/>
	
	<!-- Serial Interface -->
	<node pkg="rosserial_arduino" type="serial_node.py" name="SerialComms">
		<param name="port" value="/dev/ttyACM0"/>
		<param name="baud" value="115200"/>
	</node>
	
</launch>
